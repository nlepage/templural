{
  "name": "templural",
  "version": "1.0.0",
  "description": "Template function for plural-sensitive formatting",
  "main": "dist/cjs",
  "module": "dist/es",
  "repository": "https://github.com/nlepage/templural",
  "author": {
    "name": "Nicolas LEPAGE",
    "url": "https://github.com/nlepage"
  },
  "license": "Apache-2.0",
  "keywords": [
    "i18n",
    "internationalization",
    "template-literals",
    "plural-rules",
    "pluralize"
  ],
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig-cjs.json && tsc -p tsconfig-es.json",
    "clean": "rm -rf dist",
    "test": "ava"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^17.0.9",
    "all-contributors-cli": "^6.20.0",
    "ava": "^4.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "packageManager": "yarn@3.1.1"
}
